<h2>Best rated beers</h2>
<ul>

 <% @top_beers.each do |beer| %>
   <li> <%= beer %>  <%= link_to beer.name, beer %> </li>
 <% end %>
</ul>
<h2>Best rated breweries</h2>
<ul>

 <% @top_breweries.each do |brewery| %>
   <li> <%= brewery %>  <%= link_to brewery.name, brewery %> </li>
 <% end %>
</ul>
<h2>Best rated styles</h2>
<ul>

 <% @top_styles.each do |style| %>
   <li> <%= style.name %> </li>
 <% end %>
</ul>
<h2>Most ratings by users</h2>
<ul>

 <% @top_users.each do |user| %>
   <li> <%= user.username %>  <%= link_to user.username, user %> </li>
 <% end %>
</ul>
<h2>Latest ratings</h2>
<ul>

 <% @ratings_latest.each do |rating| %>
   <li> <%= rating %>  <%= link_to rating.user.username, rating.user %> </li>
 <% end %>
</ul>

<p><button type="button" class="btn btn-primary" <%= link_to 'New', new_rating_path %></button>  </p>
